  "geocr:temporalExtent": {
    "start": "2024-01-01T00:00:00Z",
    "end": "2024-12-31T23:59:59Z"
  },
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "point-data",
      "name": "point-data",
      "description": "Point location data",
      "contentUrl": "https://example.org/data/point-data.geojson",
      "encodingFormat": "application/geo+json",
      "sha256": "xyz789abc123"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "point-records",
      "name": "point-records",
      "description": "Point location records",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "point-records/geometry",
          "name": "geometry",
          "description": "Point geometry",
          "dataType": "cr:GeoJSON"
        },
        {
          "@type": "cr:Field",
          "@id": "point-records/properties",
          "name": "properties",
          "description": "Feature properties",
          "dataType": "sc:PropertyValue"
        }
      ]
    }
  ]
